      <ServiceGroups>
        <ServiceGroup Key="ISIASERVICE" Sequence="1">
          <Service Key="SERVICE_USERINTERFACE" DisplayName="" Sequence="2" StartTime="6" EndTime="6" ProcessCount="1" AssemblyFileName="ISIA.SERVICE" AssemblyName="ISIA.SERVICE.SvcUserInterface" Argument=""/>
