          <Service Key="SERVICE_ELSUMMARYPM2" DisplayName="" Sequence="0" StartTime="20" EndTime="20" ProcessCount="1" AssemblyFileName="ISIA.SERVICE" AssemblyName="ISIA.SERVICE.SvcELDataDailySummary" Argument=""/>
 <!--<Service Key="SERVICE_LDSUMMARY" DisplayName="" Sequence="0" StartTime="8" EndTime="8" ProcessCount="1" AssemblyFileName="ISIA.SERVICE" AssemblyName="ISIA.SERVICE.SvcLDDataDailySummary" Argument=""/>-->
