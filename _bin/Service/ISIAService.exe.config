                   ServerIP="172.16.45.93"
                   UserLanguage="KR"
                   UILogging ="false"
                   ProductName ="ISIA Service"
                   MDIName ="Service"
                   MDIDisplayName ="ISIA_Service v1.0"
                   ServiceInterval ="3600"
                   AppConfigFileName ="ISIAService.config" AppConfigName="ISIA.SERVICE.Configuration">
      <Apps>
        <App Key="MES" Sequence="1" >
          <!--<SubApp  Key="MODELER" AppDirectory="minaSolar\modeler" Image="launcher_modeler.png" HoverImage="launcher_modeler_hover.png" Argument="" AppConfg="" Enabled="true" DisplayName="Modeler" />-->
